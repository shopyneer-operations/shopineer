{
  "name": "shopyneer",
  "version": "0.0.2",
  "description": "A Prebaked Medusa 2.0 backend",
  "author": "Rasmus Puls",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node src/scripts/postBuild.js",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "ib": "init-backend",
    "start": "init-backend && medusa db:migrate --execute-all-links && cd .medusa/server && medusa start --verbose",
    "dev": "medusa develop",
    "email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002",
    "restart": "medusa db:migrate --execute-all-links && medusa user -e killua@mail.com -p killua && medusa develop"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@medusajs/admin-sdk": "2.12.1",
    "@medusajs/admin-shared": "2.12.1",
